<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape
            android:shape="rectangle">
            <corners android:radius="@dimen/box_bg_round" />
            <stroke android:width="@dimen/box_bg_stroke"
                android:color="@color/app_theme_color_2" />
            <solid android:color="@color/app_white" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <corners android:radius="@dimen/box_bg_round" />
            <stroke android:width="@dimen/box_bg_stroke"
                android:color="@color/app_theme_color_2" />
            <solid android:color="@color/app_theme_color_2" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
            <corners android:radius="@dimen/box_bg_round" />
            <stroke android:width="@dimen/box_bg_stroke"
                android:color="@color/app_theme_color_2" />
            <solid android:color="@color/app_white" />
        </shape>
    </item>
</selector>
